e.call(source, key)) continue; target[key] = source[key]; } } return target; }\n\nfunction _objectWithoutPropertiesLoose(source, excluded) { if (source == null) return {}; var target = {}; var sourceKeys = Object.keys(source); var key, i; for (i = 0; i < sourceKeys.length; i++) { key = sourceKeys[i]; if (excluded.indexOf(key) >= 0) continue; target[key] = source[key]; } return target; }\n\n/* @jsxRuntime classic */\n\n/* @jsx mdx */\nvar _frontmatter = {\n  \"title\": \"EPI2ME Labs Launcher Command-line interface\",\n  \"author\": \"Chris Wright\",\n  \"category\": \"How Tos\",\n  \"tags\": [\"#tutorial\", \"#notebooks\"],\n  \"date\": \"2021-03-17T00:00:00.000Z\",\n  \"thumbnail\": null\n};\nvar layoutProps = {\n  _frontmatter: _frontmatter\n};\nvar MDXLayout = \"wrapper\";\nreturn function MDXContent(_ref) {\n  var components = _ref.components,\n      props = _objectWithoutProperties(_ref, [\"components\"]);\n\n  return mdx(MDXLayout, _extends({}, layoutProps, props, {\n    components: components,\n    mdxType: \"MDXLayout\"\n  }), mdx(\"p\", null, \"Feedback from EPI2ME Labs users revealed the desired for people to be able to control the\\nrunning of the EPI2ME Labs notebook server from the command-line without opening a\\ngraphical user interface. Such users are typically running the server on a different,\\nand more computationall capable, computer from where they are using the notebook\\nweb interface.\"), mdx(\"p\", null, \"With the \", mdx(\"a\", {\n    parentName: \"p\",\n    \"href\": \"/downloads\"\n  }, \"v1.0.8 release\"), \" of the launcher we have introduced the ability\\nto start, stop, and check the status of the EPI2ME Labs notebook server from the\\ncommand-line. The application is install to\"), mdx(\"pre\", null, mdx(\"code\", {\n    parentName: \"pre\"\n  }, \"/usr/local/bin/EPI2ME-Labs-Launcher/EPI2ME-Labs-Launcher\\n\")), mdx(\"p\", null, \"on Linux systems. On Ubuntu systems this is aliased as two commands, \", mdx(\"inlineCode\", {\n    parentName: \"p\"\n  }, \"labslauncher\"), \" and\\n\", mdx(\"inlineCode\", {\n    parentName: \"p\"\n  }, \"labsmanager\"), \", installed to:\"), mdx(\"pre\", null, mdx(\"code\", {\n    parentName: \"pre\"\n  }, \"/usr/local/bin\\n\")), mdx(\"p\", null, \"which is typically listed in the \", mdx(\"inlineCode\", {\n    parentName: \"p\"\n  }, \"PATH\"), \" environment variable.\"), mdx(\"p\", null, \"Running the \", mdx(\"inlineCode\", {\n    parentName: \"p\"\n  }, \"EPI2ME-Labs-Launcher\"), \" (or \", mdx(\"inlineCode\", {\n    parentName: \"p\"\n  }, \"labslauncher\"), \" shortcut) will run the standard\\nEPI2ME Labs Launcher with a graphic interface, and with logging to the terminal. However\\nrunning:\"), mdx(\"pre\", null, mdx(\"code\", {\n    parentName: \"pre\"\n  }, \"/usr/local/bin/EPI2ME-Labs-Launcher/EPI2ME-Labs-Launcher manager --help\\n\")), mdx(\"p\", null, \"(or simply \", mdx(\"inlineCode\", {\n    parentName: \"p\"\n  }, \"labsmanager --help\"), \" on Ubuntu) will initiate a simple command-line interface.\\nThe interface has the following commands:\"), mdx(\"pre\", null, mdx(\"code\", {\n    parentName: \"pre\"\n  }, \"  --start      Start the server.\\n  --restart    Restart the server.\\n  --stop       Stop a running server and cleanup resources.\\n  --status     Report server status.\\n  --update     Update server.\\n\")), mdx(\"p\", null, \"all of which should be fairly self-explanatory. The program will alert users if\\nupdates are available for the server or for the program itself. The last\\ncommand in the above can be used to update the notebook server component. For\\nupdates to the Launcher application users should continue to download these\\nfrom the \", mdx(\"a\", {\n    parentName: \"p\",\n    \"href\": \"/downloads\"\n  }, \"downloads\"), \" page. (Users on Oxford Nanopore Technologies\\nsequencing devices can also upgrade the Launcher through \", mdx(\"inlineCode\", {\n    parentName: \"p\"\n  }, \"apt\"), \").\"), mdx(\"p\", null, \"When starting an EPI2ME Labs notebook server the full array of options\\navailable in the graphical application are available to be set; running the\\n\", mdx(\"inlineCode\", {\n    parentName: \"p\"\n  }, \"--help\"), \" command will display these. In contrast to the graphical interface,\\nwhen using the command-line options are not stored in the applications settings\\nfor subsequent use: users wishing to override the default value of options must\\nsupply these options every time the program is run. This provides a more\\nnatural command-line interface.\"), mdx(\"p\", null, \"We hope that users will find the new command-line interface useful and look\\nforward to feedback.\"));\n}\n;\nMDXContent.isMDXComponent = true;","keywords":null,"tags":[{"id":"70f9a057-09ee-57c8-a813-6bb46cc3c9fb","name":"#tutorial","slug":"/tag/tutorial/"},{"id":"dd6a650b-a8bc-5a0a-b66e-5df475dec6fc","name":"#notebooks","slug":"/tag/notebooks/"}],"thumbnail":null},"tagCategoryPosts":{"nodes":[{"id":"bc1f4f8b-de6b-5942-852a-f7fb3b3f8c46","title":"Running EPI2ME Labs Notebooks on Remote Computers","slug":"/running-epi2me-labs-notebooks-on-remote-computers/","link":null,"excerpt":"This blog post is a short introduction to running epi2me labs on a remote machine that you haveâ€¦","timeToRead":2,"featured":null,"thumbnailText":null,"date":"March 17, 2021","category":{"id":"8154a3ab-4e21-5339-a043-d93b6b07d8fc","name":"How Tos","slug":"/category/how-tos/","color":"#B8E986","icon":"/static/1de85bbd781531b61bf5a25b83d72d32/how-tos.svg"},"author":{"id":"cdc1b30f-811d-5a88-8539-c05e250edca2","name":"EPI2ME Labs","slug":"/author/epi2me-labs/","title":"Senior Button Pusher","description":"I'm a robot. I like to publish JupyterLab tutorials, Nextflow workflows, and blog posts.","skills":["Continuous Deployment"],"social":[{"name":"Twitter","url":"https://twitter.com/nanoporetech"},{"name":"Website","url":"https://labs.epi2me.io"}],"thumbnail":{"__typename":"ImageSharp","ImageSharp_small":{"layout":"fixe